{
  "buildCommand": "npm run build",
  "installCommand": "npm install && node ./node_modules/puppeteer/install.js",
  "version": 2,
  "outputDirectory": "dist",
  "routes": [
    {
      "src": "/(.*)",
      "dest": "src/main.ts",
      "methods": ["GET", "POST", "PUT", "DELETE", "OPTIONS"]
    }
  ]
}
