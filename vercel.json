{
  "buildCommand": "npm run build",
  "installCommand": "npm install && node ./node node_modules/puppeteer/install.js",
  "version": 2,
  "builds": [
    {
      "src": "src/main.ts",
      "use": "`@vercel/node`"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "src/main.ts",
      "methods": ["GET", "POST", "PUT", "DELETE", "OPTIONS"]
    }
  ]
}
